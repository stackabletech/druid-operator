[Unit]
Description=Stackable Operator for Apache Druid
Before=
After=network.target
[Service]
User=root
ExecStart=/opt/stackable/druid-operator/stackable-druid-operator
Restart=on-abort
StandardOutput=journal
StandardError=journal
Environment="DRUID_OPERATOR_LOG=info"
[Install]
WantedBy=multi-user.target
